{
  "name": "accede/regquiem",
  "description": "slim propel skeleton",
  "require": {
    "php": ">=7.3 || ^8.0",
    "monolog/monolog": "^1.17",
    "propel/propel": "~2.0@dev",
    "slim/php-view": "^2.2",
    "slim/slim": "^3.1",
    "slim/twig-view": "^2.5",
    "respect/validation": "^1.1",
    "slim/csrf": "^0.8.2",
    "slim/flash": "^0.4.0",
    "zeuxisoo/slim-whoops": "0.6.*",
    "phpmailer/phpmailer": "^6.4",
    "kint-php/kint-twig": "^3.0"
  },
  "autoload": {
    "classmap": [
      "src/dbclasses/"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
